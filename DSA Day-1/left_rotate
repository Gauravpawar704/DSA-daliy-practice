#include <iostream>
using namespace std;
int main()
{
    int n;
    int arr[100];

    cout << "Enter a element: ";
    cin >> n;

    cout << "Enter a numbers: ";
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    int first = arr[0];

    for (int i = 0; i < n - 1; i++)
    {
        arr[i] = arr[i + 1];
    }

    arr[n - 1] = first;

    cout << "Array after left rotate: ";
    for (int i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    }
}
